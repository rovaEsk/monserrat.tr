			});	
			
			
			$('#'+lignefactureid).find('[name^="perso["]').each(function(){
				var id = $(this).attr("id");
				var lg = lignefactureid.length + 7 ;
				var valeur = "";
				$('[name="'+$(this).attr("id")+'"]').each(function(){
					if(($(this).is(':checkbox') || $(this).is(':radio')) && $(this).closest('span').attr("class") == "checked"){
						valeur += $(this).val()+',';
					}else if(!$(this).is(':checkbox') && !$(this).is(':radio')){
						valeur += $(this).val()+',';
					}
				});
				
				valeur = valeur.substring(0, valeur.length-1);
				if (id.substring(0,lg) == 'perso['+lignefactureid+']') {
					var persoid = id.substring(lg+1,id.length-1) ;
					if(($(this).is(':checkbox') || $(this).is(':radio')) && $(this).closest('span').attr("class") == "checked"){
						data += 'perso'+persoid+'='+valeur+'&';
					}else if(!$(this).is(':checkbox') && !$(this).is(':radio')){
						data += 'perso'+persoid+'='+valeur+'&';
					}
					
				}	
			});
			
			
			$.ajax({
				url : 'commandes_ajax.php',
				type : 'POST',
				data : data, 
				dataType : 'html',
				success : function(code_html, statut){
				},
