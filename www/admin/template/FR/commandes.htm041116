		if($('#choixarticle').length){
			var target = document.querySelector('#choixarticle');
			var observer = new MutationObserver(function(mutations) {
			  mutations.forEach(function(mutation) {
			    if(mutation.addedNodes.length > 0){
			    	$('#caractNouvelArticle').html("");
			    	$('#qte_nouvel_article').show();
			    	$('#prixAuMetre_nouvel_article').show();
					$('#prix_nouvel_article').css('display', 'inline');
					$('#enregistrer_nouvel_article').show();
					$('#devise').show();
					var data = "action=chercherInfosArticle&";
					var artid = $('#choixarticle').find('div').attr('id').split('b')[1];
					data += "artid="+artid+"&";
					data += "factureid="+$('#factureid').val()+"&";
					$.ajax({
						url : 'commandes_ajax.php',
						type : 'POST',
						data : data,
						dataType : 'html',
						success : function(code_html, statut){
							var qte = code_html.split("---");
							if(qte[0] == "metre"){
								$('#prixAuMetre_nouvel_article').show();
								$('#qte_nouvel_article').hide();
							}else{
								$('#qte_nouvel_article').show();
								$('#prixAuMetre_nouvel_article').hide();
							}
							var infos = qte[1].split("!");
							var prix = infos[0];
							
							if($('#qte_nouvel_article').is(':hidden')){
								$('#prix_nouvel_article').val(parseFloat(prix)/100*parseFloat($('#prixAuMetre_nouvel_article').val()));
							}else{
								$('#prix_nouvel_article').val(parseFloat(prix)*parseFloat($('#qte_nouvel_article').val()));
							}
							$('#prix_nouvel_article').val($('#prix_nouvel_article').val().replace(".", ","));
							
							for(var i=1;i<infos.length;i++){
								var carac = infos[i].split("_");
								var first = true;
								var options = "";
								for(var j=0;j<carac.length;j++){
									var caracid = carac[j].split(":");
									
									if(first){
										var select_id = caracid[1];

										$('#caractNouvelArticle').append("<label>"+caracid[0]+" :</label><select id='caract_nouvel_article-"+select_id+"' name='caract_"+caracid[1]+"' class='form-control caract_nouvel_article'></select><br>");
										first = false;
									}else{
										if(caracid[0].substr(0,1) == "|"){
											caracid[0] = caracid[0].substr(1);
											options += "<option value='"+caracid[1]+"' selected>"+caracid[0]+"</option>";
										}else{
											options += "<option value='"+caracid[1]+"'>"+caracid[0]+"</option>";
										}
										
									}
								}
								$('#caract_nouvel_article-'+select_id).html(options);
							}
						},
						error : function(resultat, statut, erreur){
							alert(erreur);	
						}
					});
			    }else{
			    	$('#qte_nouvel_article').hide();
			    	$('#prixAuMetre_nouvel_article').hide();
					$('#prix_nouvel_article').hide();
					$('#enregistrer_nouvel_article').hide();
					$('#devise').hide();
					$('#caractNouvelArticle').html("");
			    }
			  });    
			});
			var config = { attributes: true, childList: true, characterData: true };
			observer.observe(target, config);
		}
		//Fin de l'observer
		
